{
  "name": "food",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.3.1",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.1",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.3",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "react-svg": "^14.1.3",
    "sass": "^1.43.5",
    "serve": "^13.0.2",
    "server": "^1.0.37",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prod": "npm run-script build && serve build",
    "build": "react-scripts build",
    "test": "jest -c jest.config.js --colors --passWithNoTests",
    "prepare": "husky install",
    "coverage": "jest -c jest.config.js --colors --coverage --passWithNoTests",
    "lint:sass": "sass-lint -v",
    "lint:eslint": "eslint --ext .jsx,.js ./src && eslint --ext .jsx,.js",
    "lint:prettier": "prettier -c .prettierrc.json \"src/**/*.jsx\" --check",
    "lint": "npm run lint:eslint && npm run lint:sass && npm run lint:prettier",
    "prettier": "prettier -c .prettierrc.json \"src/**/*.jsx\" --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-react": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-lodash": "^7.3.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.3.2",
    "sass-lint": "^1.13.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,sass,md,mdx}": "prettier --write"
  }
}
